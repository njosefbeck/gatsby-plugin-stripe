{
  "name": "gatsby-plugin-stripe",
  "description": "Gatsby plugin that adds stripe.js to site",
  "version": "1.2.2",
  "author": "Nathan Beck <njosefbeck@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/njosefbeck/gatsby-plugin-stripe.git"
  },
  "homepage": "https://github.com/njosefbeck/gatsby-plugin-stripe.git",
  "bugs": {
    "url": "https://github.com/njosefbeck/gatsby-plugin-stripe/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "stripe",
    "stripe.js"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir .",
    "watch": "babel -w src --out-dir .",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "lint": "eslint src --ext .js",
    "format": "prettier --write src/**/**.js"
  },
  "peerDependencies": {
    "gatsby": ">=1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "cross-env": "5.2.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.18.2"
  },
  "dependencies": {}
}
